COMMENT    = _{ "#" ~ (!"\n" ~ ANY)* }
WHITESPACE = _{ " " }

quit   = !{ "quit" | "q" }
open   = !{ "open" | "o" }
errors = !{ "errors" }
info   = !{ "info" }

page_number = !{ ASCII_DIGIT+ }

verb = !{ quit | open | errors | info | page_number }

arg = !{ (!WHITESPACE ~ ANY)+ }

line = { SOI ~ ":" ~ verb ~ arg* ~ EOI }
